{% extends 'base.html.twig' %}

{% block body %}
<div class="wrapper-form-login" style="width: 800px">
<table width="100%" class="table table-striped table-bordered table-hover table-condensed">
    <thead>
    <th>Наименование проекта</th>
    <th>Дата</th>
    </thead>
    {% for project in projects %}
    <tr>
        <td><a href="{{ path('show_project') }}?id={{ project.id }}">{{ project.name }}</a></td>
        <td>{{ project.created_on }}</td>
    </tr>
    {% endfor %}
</table>
</div>
{% endblock %}

